{
  "name": "yakima-events",
  "version": "1.0.0",
  "description": "Yakima Events Calendar and Local Shops Directory with Google Maps Integration",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "scrape": "npx tsx scripts/scrape-events.ts",
    "scrape:test": "npx tsx scripts/scrape-events.ts --dry-run"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.0",
    "drizzle-kit": "^0.24.0",
    "postcss": "^8.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.33.0",
    "jsdom": "^24.0.0",
    "mysql2": "^3.9.0",
    "zod": "^3.23.0"
  },
  "devDependencies2_note": "jsdom types for HTML parsing",
  "optionalDependencies": {
    "@types/jsdom": "^21.1.0",
    "tsx": "^4.7.0"
  }
}
